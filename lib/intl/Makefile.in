CC = @CC@
LIBTOOL = @LIBTOOL@
RANLIB = @RANLIB@
BISON = @INTLBISON@
BISONFLAGS = --name-prefix=__gettext
YACC = @INTLBISON@ -y -d
YFLAGS = --name-prefix=__gettext

ARFLAGS = @ARFLAGS@

LOCAL_DEFS = @LOCAL_DEFS@

MKDEP = $(SUPPORT_SRC)mkdep
MKDEP_CC = $(CC)
MKDEP_OPTS = -c $(MKDEP_CC) -P -S "$(srcdir)" -T "$(topdir)"
MKDEP_CCFLAGS = $(CCFLAGS) $(INCLUDES) -DIN_LIBINTL
MKDEP_CSOURCES = $(srcdir)/*.c # *.c

DEFS = -DLOCALEDIR=\"$(localedir)\" -DLOCALE_ALIAS_PATH=\"$(aliaspath)\" \
	-DLIBDIR=\"$(libdir)\" -DIN_LIBINTL -DBUILDING_LIBINTL @DEFS@

